{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "esModuleInterop": true,
    "allowJs": true
  },
  "include": ["src/**/*", "__test__/**/*.ts", "cypress/**/*.ts"],
  "exclude": ["node_modules"]
}
